{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/milanes-project/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/milanes-project/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/milanes-project",
  "projects": {
    "milanes-project": {
      "type": "application",
      "root": "apps/milanes-project",
      "entryFile": "main",
      "sourceRoot": "apps/milanes-project/src",
      "compilerOptions": {
        "tsConfigPath": "apps/milanes-project/tsconfig.app.json"
      }
    },
    "milanes": {
      "type": "application",
      "root": "apps/milanes",
      "entryFile": "main",
      "sourceRoot": "apps/milanes/src",
      "compilerOptions": {
        "tsConfigPath": "apps/milanes/tsconfig.app.json"
      }
    },
    "backoffice": {
      "type": "application",
      "root": "apps/backoffice",
      "entryFile": "main",
      "sourceRoot": "apps/backoffice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backoffice/tsconfig.app.json"
      }
    },
    "mooc": {
      "type": "application",
      "root": "apps/mooc",
      "entryFile": "main",
      "sourceRoot": "apps/mooc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mooc/tsconfig.app.json"
      }
    },
    "moocs": {
      "type": "application",
      "root": "apps/moocs",
      "entryFile": "main",
      "sourceRoot": "apps/moocs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/moocs/tsconfig.app.json"
      }
    }
  }
}